{
    "name": "markup/oembed-bundle",
    "description": "Bundle with a way of defining oEmbed providers, and rendering oEmbed in templates.",
    "keywords": ["oembed", "media"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "douglas@usemarkup.com"
        },
        {
            "name": "Markup",
            "homepage": "http://www.usemarkup.com/"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1|~3.0",
        "twig/twig": "~1.12|~2.0",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "symfony/console": "~2.1|~3.0",
        "kriswallsmith/buzz": "~0.10",
        "phpunit/phpunit": "4.*"
    },
    "suggest": {
        "symfony/console": "To use CLI OEmbed scripts.",
        "kriswallsmith/buzz": "For making HTTP requests using the Buzz client."
    },
    "autoload": {
        "psr-0": { "Markup\\OEmbedBundle": "" }
    },
    "target-dir": "Markup/OEmbedBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
